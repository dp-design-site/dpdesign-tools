
function change() {


        let results = Array.from(document.querySelectorAll('.result > div')),
            modelsChecked = document.querySelectorAll('.filter input.models:checked');
        //alert(document.querySelectorAll('.filter input.models').length);
        // Hide all results
        results.forEach(function(result) {
            result.style.display = 'none';
        });
		// show everything if nothing is selected
		let broika = document.querySelectorAll('.filter input.models:checked');
		if(broika.length < 1)
        { 
	     results.forEach(function(result) {
            result.style.display = 'block';
        });
		}			
        // Filter results to only those that meet ALL requirements:        

        filterModels(modelsChecked);
        
        //if (processorsChecked.length != 0) {
        //    filterModelsOrProcessors(processorsChecked);
        //}

        function filterModels(modelsChecked) {
            results = Array.from(modelsChecked).reduce(function(sum, input) {
                const attrib = input.getAttribute('value');
                return sum.concat(results.filter(function(result) {
                    return result.classList.contains(attrib);
                }));
            }, []);
        }
        // Show those filtered results:
        results.forEach(function(result) {
            result.style.display = 'block';
        });
    }
//change();


