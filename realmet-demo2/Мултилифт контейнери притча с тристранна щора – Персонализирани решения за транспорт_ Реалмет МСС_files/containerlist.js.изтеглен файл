function enterForm(code) {
  var elmnt = document.getElementById("verfuegbarkeitanfragen-584-text-3");
  var pyrvopole = document.getElementById("verfuegbarkeitanfragen-584-text-1"); 
  elmnt.value = code;
  elmnt.disabled = true;
  pyrvopole.scrollIntoView();
}