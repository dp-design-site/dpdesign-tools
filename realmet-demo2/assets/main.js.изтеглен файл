
$(document).ready(function() {
  $("#realmetModal").modal('show');
  var $img = $('.box img'),
    dsrc = $img.attr('src');
  $('.navigation li a').hover(function() {
    var $this = $(this).addClass('hover');
    $img.attr('src', $this.data('image'));
  }, function() {
  //  $(this).removeClass('hover');
  //  $img.attr('src', dsrc);
  });
})

